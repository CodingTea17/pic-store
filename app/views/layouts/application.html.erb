<!DOCTYPE html>
<html>
  <head>
    <title>PicStore</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render "layouts/nav" %>
    <div class="container">
      <div class="row">
        <div class="col-12">
          <div class="card card-body">
          <%= yield %>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
